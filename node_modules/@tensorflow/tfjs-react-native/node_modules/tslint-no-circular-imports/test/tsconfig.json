{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es5",
      "es2015.core",
      "es2015.collection",
      "es2015.iterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "types": [
      "node"
    ]
  }
}
